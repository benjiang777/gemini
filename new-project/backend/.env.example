# 服务器配置
PORT=3001
NODE_ENV=development

# 前端URL（用于CORS配置）
FRONTEND_URL=http://localhost:3000

# 数据库配置（如果使用MongoDB）
# MONGODB_URI=mongodb://localhost:27017/myapp

# JWT密钥（如果使用JWT认证）
# JWT_SECRET=your-super-secret-jwt-key
# JWT_EXPIRES_IN=7d

# 日志级别
LOG_LEVEL=info

# API速率限制
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100